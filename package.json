{
  "name": "skills-desktop",
  "version": "0.2.2",
  "description": "Visualize installed Skills and symlink status across AI agents",
  "main": "./out/main/index.js",
  "author": "Laststance.io",
  "homepage": "https://github.com/laststance/skills-desktop",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "ELECTRON_RUN_AS_NODE= electron-vite dev --remote-debugging-port=9222",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "build:mac": "electron-vite build && electron-builder --mac",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prettier": "prettier --ignore-unknown --write .",
    "clean": "rm -rf dist build .vite node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@laststance/redux-storage-middleware": "^0.2.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.3.9",
    "lucide-react": "^0.563.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^4.5.4",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@aspect-ratio/core": "^1.0.0",
    "@electron/notarize": "^2.5.0",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "code-inspector-plugin": "^1.4.1",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^5.0.0",
    "eslint": "^9.18.0",
    "eslint-config-ts-prefixer": "^4.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
